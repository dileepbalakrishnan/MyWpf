<Window x:Class="DispatcherDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DispatcherDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Number of primes :" HorizontalAlignment="Center" Name="txtBlCount"></TextBlock>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Background="AntiqueWhite">
            <StackPanel Orientation="Horizontal">
                <Label Content="From:" Width="50" VerticalAlignment="Center"></Label>
                <TextBox Width="100" Height="25" Margin="10" Name="txtFrom"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="To:" Width="50" VerticalAlignment="Center"></Label>
                <TextBox Width="100" Height="25" Name="txtTo" Margin="10"></TextBox>
            </StackPanel>
            <Button Content="Calculate" Width="100" Margin="10" Name="btnCalculate" Click="BtnCalculate_Click"></Button>
            <Button Content="Cancel" Width="100" Margin="10" Name="btnCancel"></Button>
        </StackPanel>
    </Grid>
</Window>
